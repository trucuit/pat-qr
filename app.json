{
  "name": "AppQr",
  "displayName": "AppQr",
  "expo": {
    "name": "AppQr",
    "slug": "AppQr",
    "scheme": "appqr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDB623"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/2ce62e0d-637b-40b9-bbbd-980722324efc"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.yoong.qr",
      "buildNumber": "1",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FDB623"
      }
    },
    "ios": {
      "icon": "./assets/images/icon.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.yoong.qr",
      "buildNumber": "5",
      "splash": {
        "image": "./assets/images/splash.png",
        "tabletImage": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FDB623"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#FDB623"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false,
            "flipper": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      "expo-font",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera to scan QR codes.",
          "enableCodeScanner": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) needs access to your photos to scan QR codes."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "2ce62e0d-637b-40b9-bbbd-980722324efc"
      },
      "packagerOpts": {
        "config": "metro.config.js"
      }
    }
  },
  "ignite": {
    "version": "9.6.3"
  }
}

{
  "cli": {
    "version": ">= 3.15.1"
  },
  "build": {
    "development": {
      "extends": "production",
      "distribution": "internal",
      "developmentClient": true,
      "node": "18.19.0",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk",
        "ndk": "25.1.8937393"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "simulator": true
      },
      "channel": "development"
    },
    "development:device": {
      "extends": "development",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": false
      },
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk",
        "ndk": "25.1.8937393"
      },
      "channel": "development"
    },
    "preview": {
      "extends": "production",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "preview:device": {
      "extends": "preview",
      "ios": {
        "simulator": false
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "cocoapods": "1.15.2"
      },
      "android": {
        "buildType": "apk",
        "ndk": "25.1.8937393"
      },
      "autoIncrement": true,
      "channel": "production",
      "node": "20.9.0"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "gnoc.congnv@gmail.com",
        "ascAppId": "6502861841",
        "appleTeamId": "7LD785N334"
      }
    }
  }
